<div id='random-beer'>
  {{#with beer}}
    <img src='{{image_url}}' alt='{{name}}' width='100' />
    <h2> <strong>{{name}}</strong> </h2>
    <p id="random-beer-tagline">{{tagline}}</p>
    <p> {{description}} </p>
    <p> <strong>Food Pairing:</strong></p>
    <ul>
      {{#each food_pairing}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
    <p><strong>Brewer Tips:</strong>{{brewers_tips}}</p>
  {{/with}}
</div>